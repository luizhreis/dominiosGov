% layout 'layout';
% title 'DominiosGov';

<div class="well bs-component">
	<label for="btn6">Listagem da quantidade de dom√≠nios sob responsabilidade de uma mesma empresa: </label>
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Empresa</th>
				<th>Quantidade</th>
			</tr>
		</thead>
		<tbody>
