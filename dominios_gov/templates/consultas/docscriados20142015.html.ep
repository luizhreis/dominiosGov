% layout 'layout';
% title 'DominiosGov';

<div class="well bs-component">
	<label for="btn5">Listagem do número de documento das empresas responsáveis por domínios criados durante os anos 2014 e 2015: </label>
	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<th>Documento</th>
			</tr>
		</thead>
		<tbody>
			<% for my $item (@$itens) { %>
				<tr scope="row">
				<td><%= $item->[0] %></td>
				</tr>
			<% } %>
		</tbody>
	</table>
</div>
